<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:ws="http://ws.engine.ck.com/">
    <soapenv:Header>
        <Usuario xmlns="uri:com.ck.engine.ws" xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/">$username</Usuario>
        <Password xmlns="uri:com.ck.engine.ws" xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/">$password</Password>
    </soapenv:Header>
   <soapenv:Body>
      <ws:requestCancelarOutput>
         <cancelarOutputBean>
            <elementos>
               <idPedido>$id</idPedido>
               <idCuenta>$accountId</idCuenta>
            </elementos>
         </cancelarOutputBean>
      </ws:requestCancelarOutput>
   </soapenv:Body>
</soapenv:Envelope>